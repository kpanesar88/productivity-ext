{
    "name": "Productivity Tracker",
    "version": "1.0.0",
    "description": "This is an extension that uses a camera to detect productivity.",
    "manifest_version": 3,
    "permissions": [
      "activeTab",
      "camera"
    ],
    "author": "Karanveer Panesar",
    "action": {
      "default_popup": "index.html",
      "default_title": "Productivity Tracker"
    },

    "options_page": "index.html",

    "icons": {
      "16": "blueeye.png",
      "48": "blueeye.png",
      "128": "rainboweye.jpg"
    }
  }



  